for var num in 1...5 {
    IO.Println(num)
}

IO.Println("---")

for var item in [1, 2, 3, "sista"] {
	IO.Println(item)
}

IO.Println("---")

for var fniss in [1000, 2000, 3000, 4000, 5000] {
	IO.Println(fniss)	
}

IO.Println("---")

for var a = 0; a < 10; a++ {
	IO.Println(a)
}